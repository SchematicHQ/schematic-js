{
  "dependencyTypes": ["dev", "prod"],
  "customTypes": {
    "engines": {
      "path": "engines",
      "strategy": "versionsByName"
    },
    "nodeEngine": {
      "path": "engines.node",
      "strategy": "version"
    },
    "packageManager": {
      "path": "packageManager",
      "strategy": "name@version"
    }
  }
}
