{
  "dependencies": {
    "@types/uuid": "^9.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.2",
    "@parcel/transformer-typescript-types": "^2.9.2",
    "parcel": "^2.9.2",
    "typescript": "^5.0.2"
  },
  "modules": false,
  "name": "@schematichq/schematic-js",
  "publishConfig": {
    "@schematichq:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "parcel build src/browser.ts --public-url ./ --detailed-report",
    "clean": "rm -rf dist && rm -rf .parcel-cache",
    "watch": "parcel watch src/browser.ts --public-url ./"
  },
  "main": "src/main.ts",
  "source": "src/main.ts",
  "targets": {
    "default": {
      "isLibrary": false,
      "context": "browser",
      "sourceMap": false,
      "outputFormat": "global"
    }
  },
  "types": "src/main.d.ts",
  "version": "0.0.1"
}
